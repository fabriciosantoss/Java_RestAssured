{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "data": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "bank": {
          "type": "string"
        },
        "accountNumero": {
          "type": "string"
        }
      },
      "required": [
        "id",
        "bank",
        "accountNumero"
      ]
    },
    "links": {
      "type": "object",
      "properties": {
        "self": {
          "type": "string"
        }
      },
      "required": [
        "self"
      ]
    },
    "meta": {
      "type": "object",
      "properties": {
        "totalRecords": {
          "type": "integer"
        },
        "totalPages": {
          "type": "integer"
        },
        "requestDateTime": {
          "type": "number"
        }
      },
      "required": [
        "totalRecords",
        "totalPages",
        "requestDateTime"
      ]
    }
  },
  "required": [
    "data",
    "links",
    "meta"
  ]
}